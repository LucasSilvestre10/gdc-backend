{
  "name": "gdc-backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest --coverage",
    "build": "npm run barrels && swc src --out-dir dist -s  --strip-leading-paths",
    "fix-dist-imports": "node ./tools/fix-dist-imports.cjs",
    "build-and-fix": "npm run build && npm run fix-dist-imports",
    "barrels": "barrels",
    "start": "npm run barrels && nodemon src/index.ts",
    "start-db": "docker compose -f docker-compose-db.yml up --build -d",
    "start-app": "docker compose -f docker-compose-app.yml up --build -d",
    "logs": "docker logs -f gdc-container-app",
    "start:prod": "cross-env NODE_ENV=production node --import @swc-node/register/esm-register src/index.js",
    "seed": "node ./tools/import-seeds.cjs",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "lint:check": "eslint --ext .ts src/ --max-warnings 0",
    "lint:unused": "ts-unused-exports tsconfig.json --showLineNumber --excludePathsFromReport=test;spec;coverage",
    "lint:typescript": "tsc --noEmit",
    "lint:all": "npm run lint && npm run lint:unused && npm run lint:typescript"
  },
  "dependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@tsed/ajv": "^8.16.1",
    "@tsed/barrels": "^6.6.3",
    "@tsed/core": "^8.16.1",
    "@tsed/di": "^8.16.1",
    "@tsed/engines": "^8.16.1",
    "@tsed/exceptions": "^8.16.1",
    "@tsed/json-mapper": "^8.16.1",
    "@tsed/logger": "^8.0.4",
    "@tsed/mongoose": "^8.16.1",
    "@tsed/openspec": "^8.16.1",
    "@tsed/platform-cache": "^8.16.1",
    "@tsed/platform-exceptions": "^8.16.1",
    "@tsed/platform-express": "^8.16.1",
    "@tsed/platform-http": "^8.16.1",
    "@tsed/platform-log-request": "^8.16.1",
    "@tsed/platform-middlewares": "^8.16.1",
    "@tsed/platform-multer": "^8.16.1",
    "@tsed/platform-params": "^8.16.1",
    "@tsed/platform-response-filter": "^8.16.1",
    "@tsed/platform-views": "^8.16.1",
    "@tsed/schema": "^8.16.1",
    "@tsed/swagger": "^8.16.1",
    "ajv": "^8.17.1",
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "dotenv-expand": "^12.0.3",
    "dotenv-flow": "^4.1.0",
    "express": "^5.1.0",
    "method-override": "^3.0.0",
    "mongoose": "^8.18.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tsed/cli-plugin-eslint": "6.6.3",
    "@tsed/cli-plugin-mongoose": "6.6.3",
    "@tsed/cli-plugin-vitest": "6.6.3",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/method-override": "^3.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-unused-exports": "^11.0.1",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "npm",
    "runtime": "node"
  },
  "type": "module"
}
